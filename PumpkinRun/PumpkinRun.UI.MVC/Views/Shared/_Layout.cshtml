            PumpkinRun.DATA.EF.Models.db_a88655_pumpkinrunContext _context = new PumpkinRun.DATA.EF.Models.db_a88655_pumpkinrunContext();
            var sponsors = _context.Sponsors.ToList();






            <section id="our-team">
                <div class="section-inner">
                    <div class="container">
                        <div class="row mb60 text-center">
                            <div class="col-sm-12">
                                <h3 class="section-title">Meet Our Sponsors</h3>
                                @* <p class="section-sub-title">The very best in their fields.</p>*@
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-12">
                                <div id="music-carousel" class="carousel slide">
                                    <div class="carousel-inner">
                                        <div class="item active">
                                            <div class="row" style="background-color: white">

                                                @foreach (var item in sponsors.Take(4))
                                                {
                                                    <div class="col-md-3">
                                                        <div class="hover-effect smoothie">
                                                            <a class="thumbnail smoothie">
                                                                <img src="~/assets/images/sponsors/@item.SponsorLogo" alt="sponsor logo" class="smoothie mt-5" style="max-height: 7em;">
                                                            </a>

                                                        </div>
                                                    </div>
                                                }

                                            </div>
                                        </div>
                                        @for (var i = 4; i <= sponsors.Count(); i += 4)
                                        {
                                            <div class="item">
                                                <div class="row" style="background-color: white">


                                                    @foreach (var item in sponsors.Skip(i).Take(4))
                                                    {
                                                        <div class="col-md-3">
                                                            <div class="hover-effect smoothie">
                                                                <a class="thumbnail smoothie">
                                                                    <img src="~/assets/images/sponsors/@item.SponsorLogo" alt="sponsor logo" class="smoothie mt-5" style="max-height: 7em;">
                                                                </a>

                                                            </div>
                                                        </div>
                                                    }

                                                </div>
                                            </div>
                                        }
                                    </div>
                                    <a data-slide="prev" href="#music-carousel" class="left carousel-control"><i class="fa fa-long-arrow-left"></i></a>
                                    <a data-slide="next" href="#music-carousel" class="right carousel-control"><i class="fa fa-long-arrow-right"></i></a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>


        }

