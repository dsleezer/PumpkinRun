@model PumpkinRun.DATA.EF.Models.SponsorTypeBenefit

@{
    ViewData["Title"] = "Delete";
}

<section class="silver-wrapper">
    <div class="section-inner-60">
        <div class="container">
            <div class="row cta">

                <div class="col-sm-9">
                    <h4 class="text-danger">Are you sure you want to delete this sponsor type benefit?</h4>
                </div>
<div>
   
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Benefit)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Benefit.BenefitDescription)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.SponsorType)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.SponsorType.SponsorTypeName)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Stbid" />
        <input type="submit" value="Delete" class="btn btn-default" /> |
        <a asp-action="Index">Back to Sponsor Type Benefits</a>
    </form>
</div>
</div>
</div>
</div>
</section>